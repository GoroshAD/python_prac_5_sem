r = Triangle((4, 2), (4, 2), (2, 4))
s = Triangle((0, 2), (2, 6), (5, 1))
t = Triangle((1, 3), (2, 4), (4, 2))
o = Triangle((1, 0), (2, 0), (3, 6))
print(*(f"{n}({bool(x)}):{round(abs(x), 3)}" for n, x in zip("rsto", (r, s, t, o))))
print(f"{s < t=}, {o < t=}, {r < t=}, {r < s=}")
print(f"{s in t=}, {o in t=}, {r in t=}")
print(f"{r & t=}, {t & r=}, {s & t=}, {o & t=}")
